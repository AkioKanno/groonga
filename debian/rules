#!/usr/bin/make -f
# -*- makefile-gmake -*-
#
# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1
# This has to be exported to make some magic below work.
export DH_OPTIONS

%:
	dh --with quilt $@

# override_dh_auto_configure:
#	dh_auto_configure -- --xxx=zzz

override_dh_install:
	install -m 0644 debian/groonga-munin-plugin.conf \
	  debian/groonga/etc/munin/plugin-conf.d/groonga
	dh_install
