table_create Geo TABLE_HASH_KEY ShortText
column_create Geo distance COLUMN_SCALAR Int32

load --table Geo
[
{"_key": "the record for geo_distance() result"}
]

# from (longitude -180 latitude 1) to (longitude 90 latitude 89)
