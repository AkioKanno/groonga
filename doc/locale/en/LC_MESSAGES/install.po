msgid "CentOS 5"
msgstr "CentOS 5"

#: ../../../source/install.txt:101
msgid ""
"% sudo rpm -ivh http://packages.groonga.org/centos/groonga-"
"repository-1.0.0-0.noarch.rpm % sudo yum update % sudo yum install -y groonga"
msgstr ""
"% sudo rpm -ivh http://packages.groonga.org/centos/groonga-"
"repository-1.0.0-0.noarch.rpm % sudo yum update % sudo yum install -y groonga"

